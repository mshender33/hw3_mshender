<h1><%= @place.name %></h1>

<h2>Entries</h2>
<ul>
  <% if @place.entries.any? %>
    <% @place.entries.each do |entry| %>
      <li>
        <strong><%= entry.title %></strong>
        <p><%= entry.content %></p>
      </li>
    <% end %>
  <% else %>
    <p>No entries available for this place.</p>
  <% end %>
</ul>
